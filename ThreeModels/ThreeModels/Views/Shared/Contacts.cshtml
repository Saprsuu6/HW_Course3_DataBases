@model Contacts

<dl class="dl-horizontal">
    <dt>@Localizer["Email"]</dt>
    <dd>@Model.Email</dd>

    <dt>@Localizer["Location"]</dt>
    @foreach(Phone phone in Model.Phones)
    {
        <dd>@phone.Number</dd>
    }

    <dt>@Localizer["Phones"]</dt>
    <dd>@Model.CountryTownStreet</dd>
</dl>
